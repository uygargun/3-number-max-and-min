#include <stdio.h>
#include <conio.h>
int main(void){
	
	int num1,num2,num3;
	printf("Enter 3 numbers: "); scanf("%d%d%d",&num1,&num2,&num3);
	if(num1>num2){
		if(num1>num3){
			printf("Max is %d\n",num1);
		}
		else{
			printf("Max is %d\n",num3);
		}
		if(num2<num3){
			printf("Min is %d\n",num2);
		}
		else{
			printf("Min is %d\n",num3);
		}
	}
	
	else{
		if(num2>num3){
			printf("Max is %d\n",num2);
		}
		else{
			printf("Max is %d\n",num3);
		}
		if(num1<num3){
			printf("Min is %d\n",num1);
		}
		else{
			printf("Min is %d\n",num3);
		}
	}
	
	
	
}
